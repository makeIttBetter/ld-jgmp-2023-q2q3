def getBuild = { -&gt;
    Thread.currentThread()?.executable
}

def setEnv = { key, value -&gt;
    def build = getBuild()
    def e = new hudson.EnvVars()
    e.put(key, value)
    build.environments.add(Environment.create(e))
}

def getRunId = { -&gt;
    def build = getBuild()
    def parameters = build?.actions.find { it instanceof ParametersAction }?.parameters
    parameters[0].runId
}
def version = getRunId().split(&apos;#&apos;)[1]

setEnv(&quot;BUILD_VESRION&quot;, version)</script>
